		"reportParseExceptions": true
	},
	"ioConfig": {
		"topic": "unauthenticated.DCAE_CL_OUTPUT",
		"replicas": 1,
		"startDelay": "PT1S",
		"taskDuration": "PT1H",
		"completionTimeout": "PT30M",
		"consumerProperties": {
			"bootstrap.servers": "message-router-kafka:9092"
		},
		"useEarliestOffset": true
	}
