							"name": "from",
							"expr": "$.from"
						}
					]
				},
				"timestampSpec": {
					"column": "closedLoopAlarmStart",
					"format": "millis"
				},
				"dimensionsSpec": {
					"dimensions": [],
					"dimensionsExclusions": [
						"cambria.partition",
						"password"
					]
				}
			}
		},
		"metricsSpec": [],
