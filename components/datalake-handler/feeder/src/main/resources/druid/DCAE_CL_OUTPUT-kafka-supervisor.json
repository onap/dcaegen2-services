						{
							"type": "path",
							"name": "closedLoopEventStatus",
							"expr": "$.closedLoopEventStatus"
						},
						{
							"type": "path",
							"name": "version",
							"expr": "$.version"
						},
						{
							"type": "path",
							"name": "closedLoopControlName",
							"expr": "$.closedLoopControlName"
						},
						{
							"type": "path",
							"name": "target",
							"expr": "$.target"
