						},
						{
							"type": "path",
							"name": "policyScope",
							"expr": "$.policyScope"
						},
						{
							"type": "path",
							"name": "target_type",
							"expr": "$.target_type"
						},
						{
							"type": "path",
							"name": "AAI:serviceType",
							"expr": "$.AAI.serviceType"
						},
						{
							"type": "path",
							"name": "AAI:service-instance_service-instance-id",
